#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
    string s;
    cin>>s;
    int count=0;
    for(int i=0;i<s.size();i++){
        if(s[i]>='0' and s[i]<='9') count;
        else count++;
    }
    if(count==0) cout<<"Yes"<<endl;
    else cout<<"No"<<endl;
    }
    return 0;
}
