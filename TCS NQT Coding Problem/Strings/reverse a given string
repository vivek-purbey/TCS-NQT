//M-I:
#include<bits/stdc++.h>
using namespace std;
int main(){
    string s;
    getline(cin,s);
    reverse(s.begin(), s.end()); //reverse the given string
    cout<<s<<endl;
    return 0;
}// otherwise like reverse of array.
//T.C:O(n) & S.C:O(1)

//M-II:
#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
    string s;
    cin>>s;
    int n=s.size();
    for(int i=n-1;i>=0;i--){
        cout<<s[i];
    }
    cout<<endl;
    }
    return 0;
}
