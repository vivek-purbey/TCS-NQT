#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cout<<"Please enter number of test cases : ";
    cin>>t;
    while(t--){
        int n;
        cout<<"Please enter the size of Array : ";
        cin>>n;
        cout<<"Please enter the elements of Array : ";
        int a[n],sum=0;
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
            sum=sum+a[i];
        }
        cout<<"Sum of this Array : "<<sum<<endl;
        cout<<endl;
    }
    return 0;
}
// Time Complexity->O(n) and Space Complexity->O(1).
